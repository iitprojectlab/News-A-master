package newspapers;

public class Headline {

	public Headline(String heading, String url) {
		setHeading(heading);
		setUrl(url);
	}

	public String getHeading() {
		return Heading;
	}
	public void setHeading(String heading) {
		Heading = heading;
	}

	public String getUrl() {
		return Url;
	}

	public void setUrl(String url) {
		Url = url;
	}

	private String Heading;
	private String Url;
}
